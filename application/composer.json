{
  "name": "jason-napolitano/codeigniter3-rest-boilerplate",
  "description": "Full Codeingiter 3 REST API Boilerplate",
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "chriskacerguis/codeigniter-restserver": "3.0.*",
    "codeigniter/framework": "3.1.*",
    "firebase/php-jwt": "5.*",
    "graylog2/gelf-php": "1.6.*",
    "lcobucci/jwt": "3.2.*",
    "luthier/luthier": "^1.0",
    "monolog/monolog": "^1.23",
    "nesbot/carbon": "2.10.*",
    "vlucas/phpdotenv": "3.1.*"
  },
  "require-dev": {
    "crazycodr/standard-exceptions": "2.3.*",
    "filp/whoops": "2.3.*",
    "fzaninotto/faker": "1.8.*",
    "mikey179/vfsStream": "1.1.*",
    "symfony/console": "^4.2",
    "symfony/var-dumper": "4.2.*"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Entities\\": "./database/entities",
      "Seeds\\": "./database/seeds"
    }
  }
}
